{
  "name": "annotated-graphql",
  "version": "0.0.5",
  "description": "Annotated GraphQL",
  "main": "dist/index.js",
  "scripts": {
    "dev": "mocha src --recursive --require should --compilers js:babel-register --watch",
    "dev:server": "babel src --ignore *.spec.js --out-dir ../annotated-graphql-server/node_modules/annotated-graphql/dist --watch",
    "test": "mocha src --recursive --require should --compilers js:babel-register",
    "dist": "rm -rf dist && npm test && babel src --ignore *.spec.js --out-dir dist",
    "prepublish": "npm run dist"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "GraphQL",
    "Annotations"
  ],
  "author": "Alberto Mijares <almilo@almilo.es>",
  "license": "MIT",
  "dependencies": {
    "graphql-tools": "^0.5.0",
    "request": "^2.69.0"
  },
  "peerDependencies": {
    "graphql": "^0.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "mocha": "^2.4.5",
    "should": "^8.2.2",
    "sinon": "^1.17.3"
  }
}
